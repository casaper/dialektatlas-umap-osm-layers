{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/UMap2",
  "definitions": {
    "UMap2": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string"
        },
        "geometry": {
          "$ref": "#/definitions/UMap2Geometry"
        },
        "properties": {
          "$ref": "#/definitions/UMap2Properties"
        },
        "uri": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": ["https"]
        },
        "layers": {
          "type": "array",
          "items": {
            "$ref": "./uMap2Layer.json"
          }
        }
      },
      "required": ["geometry", "layers", "properties", "type", "uri"],
      "title": "UMap2"
    },
    "UMap2Geometry": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string"
        },
        "coordinates": {
          "type": "array",
          "items": {
            "type": "number"
          }
        }
      },
      "required": ["coordinates", "type"],
      "title": "UMap2Geometry"
    },
    "Layer": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string"
        },
        "features": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Feature"
          }
        },
        "_umap_options": {
          "$ref": "#/definitions/LayerUmapOptions"
        }
      },
      "required": ["_umap_options", "features", "type"],
      "title": "Layer"
    },
    "LayerUmapOptions": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "rank": {
          "type": "integer"
        },
        "fields": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Field"
          }
        },
        "editMode": {
          "type": "string"
        },
        "labelKey": {
          "type": "string"
        },
        "browsable": {
          "type": "boolean"
        },
        "inCaption": {
          "type": "boolean"
        },
        "remoteData": {
          "type": "object"
        },
        "description": {
          "type": "string"
        },
        "displayOnLoad": {
          "type": "boolean"
        },
        "showLabel": {
          "type": "null"
        }
      },
      "required": [
        "browsable",
        "description",
        "displayOnLoad",
        "editMode",
        "fields",
        "inCaption",
        "name",
        "rank",
        "remoteData"
      ],
      "title": "LayerUmapOptions"
    },
    "Field": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "key": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": ["key", "type"],
      "title": "Field"
    },
    "Feature": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "$ref": "#/definitions/FeatureType"
        },
        "geometry": {
          "$ref": "#/definitions/FeatureGeometry"
        },
        "properties": {
          "$ref": "#/definitions/FeatureProperties"
        },
        "id": {
          "type": "string"
        }
      },
      "required": ["geometry", "id", "properties", "type"],
      "title": "Feature"
    },
    "FeatureGeometry": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "$ref": "#/definitions/GeometryType"
        },
        "coordinates": {
          "type": "array",
          "items": {
            "type": "array",
            "items": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "number"
                }
              }
            }
          }
        }
      },
      "required": ["coordinates", "type"],
      "title": "FeatureGeometry"
    },
    "FeatureProperties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "fid": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "_umap_options": {
          "$ref": "#/definitions/PropertiesUmapOptions"
        }
      },
      "required": ["_umap_options", "fid", "name"],
      "title": "FeatureProperties"
    },
    "PropertiesUmapOptions": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "fillColor": {
          "type": "string"
        },
        "popupShape": {
          "type": "string"
        }
      },
      "required": ["fillColor"],
      "title": "PropertiesUmapOptions"
    },
    "UMap2Properties": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "tags": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "zoom": {
          "type": "integer"
        },
        "center": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "lat": {
              "type": "number"
            },
            "lng": {
              "type": "number"
            }
          },
          "required": ["lat", "lng"]
        },
        "easing": {
          "type": "boolean"
        },
        "fields": {
          "type": "array",
          "items": {}
        },
        "licence": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "url": {
              "type": "string",
              "format": "uri",
              "qt-uri-protocols": ["https"]
            },
            "name": {
              "type": "string"
            }
          },
          "required": ["name", "url"],
          "title": "Licence"
        },
        "miniMap": {
          "type": "boolean"
        },
        "overlay": {
          "type": "object"
        },
        "showLabel": {
          "type": "null"
        },
        "slideshow": {
          "type": "object"
        },
        "tilelayer": {
          "type": "object"
        },
        "captionBar": {
          "type": "boolean"
        },
        "longCredit": {
          "type": "string"
        },
        "description": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": ["https"]
        },
        "homeControl": {
          "type": "boolean"
        },
        "limitBounds": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "east": {
              "type": "number"
            },
            "west": {
              "type": "number"
            },
            "north": {
              "type": "number"
            },
            "south": {
              "type": "number"
            }
          },
          "required": ["east", "north", "south", "west"],
          "title": "LimitBounds"
        },
        "moreControl": {
          "type": "boolean"
        },
        "onLoadPanel": {
          "type": "string"
        },
        "shortCredit": {
          "type": "string"
        },
        "zoomControl": {
          "type": "boolean"
        },
        "embedControl": {
          "type": "null"
        },
        "printControl": {
          "type": "null"
        },
        "scaleControl": {
          "type": "boolean"
        },
        "locateControl": {
          "type": "null"
        },
        "captionControl": {
          "type": "boolean"
        },
        "measureControl": {
          "type": "null"
        },
        "permanentCredit": {
          "type": "string"
        },
        "scrollWheelZoom": {
          "type": "boolean"
        },
        "editinosmControl": {
          "type": "null"
        },
        "labelInteractive": {
          "type": "boolean"
        },
        "datalayersControl": {
          "type": "boolean"
        },
        "fullscreenControl": {
          "type": "null"
        },
        "displayPopupFooter": {
          "type": "boolean"
        }
      },
      "required": [
        "captionBar",
        "captionControl",
        "center",
        "datalayersControl",
        "description",
        "displayPopupFooter",
        "easing",
        "editinosmControl",
        "embedControl",
        "fields",
        "fullscreenControl",
        "homeControl",
        "labelInteractive",
        "licence",
        "limitBounds",
        "locateControl",
        "longCredit",
        "measureControl",
        "miniMap",
        "moreControl",
        "name",
        "onLoadPanel",
        "overlay",
        "permanentCredit",
        "printControl",
        "scaleControl",
        "scrollWheelZoom",
        "shortCredit",
        "showLabel",
        "slideshow",
        "tags",
        "tilelayer",
        "zoom",
        "zoomControl"
      ],
      "title": "UMap2Properties"
    },
    "GeometryType": {
      "type": "string",
      "enum": ["MultiPolygon"],
      "title": "GeometryType"
    },
    "FeatureType": {
      "type": "string",
      "enum": ["Feature"],
      "title": "FeatureType"
    }
  }
}
